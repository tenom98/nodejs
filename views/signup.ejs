    <script>
        $(function(){
            $("#success").hide()
            $("#danger").hide()
            $("#signup").attr("disabled", "disabled")
            $("input").keyup(function(){
                var pass1 = $("#INPUT_password").val()
                var pass2 = $("#CONFIRM_password").val()
                if(pass1 && pass2){
                 if(pass1 == pass2){
                    $("#success").show()
                    $("#danger").hide()
                    $("#signup").removeAttr("disabled")
                 }else{
                    $("#success").hide()
                    $("#danger").show()
                    $("#signup").attr("disabled", "disabled")
                    }
                }else{
                    $("#success").hide()
                    $("#danger").show()
                    $("#signup").attr("disabled", "disabled")
            }
        })
        })
    </script>
